<div class="card">
  <div class="card-body">
    <h5 class="card-title">
      <%= @match.competition.name %>
      <%= @match.category %>
      <%= @match.name %>
      <%= @match.event %>
    </h5>
    <ul class="list-group list-group-flush">
    <% @entries.each do |entry| %>
      <li class="list-group-item">
        <%= entry.user.number %>
        <%= entry.user.name %>
      </li>
    <% end %>
    </ul>
  </div>
</div>